build $builddir/_coder_Markov_Switching_api.o : cc $root/interface/_coder_Markov_Switching_api.c
build $builddir/_coder_Markov_Switching_mex.o : cc $root/interface/_coder_Markov_Switching_mex.c
build $builddir/eml_int_forloop_overflow_check.o : cc $root/eml_int_forloop_overflow_check.c
build $builddir/xgetrf.o : cc $root/xgetrf.c
build $builddir/chol.o : cc $root/chol.c
build $builddir/vec.o : cc $root/vec.c
build $builddir/xgeqp3.o : cc $root/xgeqp3.c
build $builddir/gammaln.o : cc $root/gammaln.c
build $builddir/applyScalarFunctionInPlace.o : cc $root/applyScalarFunctionInPlace.c
build $builddir/div.o : cc $root/div.c
build $builddir/Markov_Switching_emxutil.o : cc $root/Markov_Switching_emxutil.c
build $builddir/Markov_Switching_mexutil.o : cc $root/Markov_Switching_mexutil.c
build $builddir/_coder_Markov_Switching_info.o : cc $root/interface/_coder_Markov_Switching_info.c
build $builddir/c_mexapi_version.o : cc $matlabdir/extern/version/c_mexapi_version.c

# Link
build $root/Markov_Switching_mex.mexmaci64 : link $builddir/Markov_Switching_data.o $builddir/rt_nonfinite.o $builddir/Markov_Switching_initialize.o $builddir/Markov_Switching_terminate.o $builddir/Markov_Switching.o $builddir/kron.o $builddir/median.o $builddir/quickselect.o $builddir/quantile.o $builddir/power.o $builddir/assertCompatibleDims.o $builddir/eye.o $builddir/eml_mtimes_helper.o $builddir/mtimes.o $builddir/combineVectorElements.o $builddir/inv.o $builddir/repmat.o $builddir/norm.o $builddir/warning.o $builddir/randn.o $builddir/diff.o $builddir/randg.o $builddir/rand.o $builddir/mpower.o $builddir/sum.o $builddir/sumMatrixIncludeNaN.o $builddir/mrdivide_helper.o $builddir/infocheck.o $builddir/gamrnd.o $builddir/discret1.o $builddir/indexShapeCheck.o $builddir/find.o $builddir/lnpdfmvn.o $builddir/det.o $builddir/qrsolve.o $builddir/logDirichlet.o $builddir/diag.o $builddir/mean.o $builddir/cov.o $builddir/_coder_Markov_Switching_api.o $builddir/_coder_Markov_Switching_mex.o $builddir/eml_int_forloop_overflow_check.o $builddir/xgetrf.o $builddir/chol.o $builddir/vec.o $builddir/xgeqp3.o $builddir/gammaln.o $builddir/applyScalarFunctionInPlace.o $builddir/div.o $builddir/Markov_Switching_emxutil.o $builddir/Markov_Switching_mexutil.o $builddir/_coder_Markov_Switching_info.o $builddir/c_mexapi_version.o 
